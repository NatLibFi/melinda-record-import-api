  "version": "1.0.7",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node src/index.mjs",
    "lint": "eslint ./src",
    "lint:dev": "eslint --fix ./src",
